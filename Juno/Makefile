ifeq ($(config),debug)
  RESCOMP = windres
  TARGETDIR = ../Bin/Debug
  TARGET = $(TARGETDIR)/libJuno.a
  OBJDIR = ../Bin/Int/Debug/Juno
  PCH = Include/jnpch.h
  GCH = $(OBJDIR)/$(notdir $(PCH)).gch
  DEFINES += -D_CRT_SECURE_NO_WARNINGS -DGLFW_INCLUDE_NONE -DJN_BUILD_DEBUG
  INCLUDES += -ISrc -IInclude -I. -IVendor/jpl -IVendor/stb -IVendor/spdlog/include -IVendor/glm -IVendor/imgui -IVendor/glad/include -IVendor/glfw/include
  FORCE_INCLUDE += -include $(OBJDIR)/$(notdir $(PCH))
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -g
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -g -std=c++17
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += ../Bin/Debug/libGLAD.a ../Bin/Debug/libGLFW.a
  LDDEPS += ../Bin/Debug/libGLAD.a ../Bin/Debug/libGLFW.a
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib64 -m64
  LINKCMD = $(AR) -rcs "$@" $(OBJECTS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
  endef
all: prebuild prelink $(TARGET)
	@:

endif

